{
  "name": "Management-Zone-Name",
  "rules": [
    {
      "type": "HOST",
      "enabled": true,
      "propagationTypes": [
        "HOST_TO_PROCESS_GROUP_INSTANCE"
      ],
      "conditions": [
        {
          "key": {
            "attribute": "HOST_GROUP_NAME"
          },
          "comparisonInfo": {
            "type": "STRING",
            "operator": "EQUALS",
            "value": "DEVELOPMENT",
            "negate": false,
            "caseSensitive": false
          }
        }
      ]
    }
  ]
}